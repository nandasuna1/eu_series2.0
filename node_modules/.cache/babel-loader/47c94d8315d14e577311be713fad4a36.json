rs;\r\n                this.tipo = res.data.Type;\r\n                this.year = res.data.Year;\r\n                this.title = res.data.Title;\r\n                this.imgUrl = res.data.Poster;\r\n                console.log(this.atores);\r\n                console.log('de midia1')\r\n                })\r\n        }\r\n}\r\n\r\n</script>\r\n\r\n\r\n<style>\r\n.card{\r\n    height: 110%;\r\n}\r\n\r\n.card-image{\r\n    size: 50%;\r\n}\r\n\r\n.content{\r\n    margin: 1% 5%;\r\n}\r\n#main{\r\n    margin-top: 5%;\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}