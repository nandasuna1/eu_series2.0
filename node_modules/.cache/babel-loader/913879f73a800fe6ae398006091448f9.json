      this.mostrar = false\r\n            }else{\r\n                this.mostrar = true\r\n            }\r\n            var key = \"&apikey=b2f3b050\";\r\n            axios.get(\"https://www.omdbapi.com/?i=\"+this.imdbID+key).then(res => {\r\n                this.plot = res.data.Plot;\r\n                this.nota = res.data.Ratings[0].Value;\r\n                this.atores = res.data.Actors;\r\n            })\r\n\r\n        }\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.card{\r\n    height: 110%;\r\n}\r\n\r\n.card-image{\r\n    size: 50%;\r\n}\r\n\r\n.content{\r\n    margin: 1% 5%;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}