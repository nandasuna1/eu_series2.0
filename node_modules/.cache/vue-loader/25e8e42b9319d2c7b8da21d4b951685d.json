Midia :year=\"midi.Year\" :title=\"midi.Title\" :imgUrl=\"midi.Poster\" :imdbID=\"midi.imdbID\" :url=\"midi.url\" :plot=\"midi.Plot\" />\r\n      <router-link :to=\"{name: 'Midia', params: {imdbID: midi.imdbID}}\">\r\n        <button class=\"button is-primary\" >INFO</button>\r\n      </router-link>\r\n    </div>\r\n  </div>\r\n  \r\n</template>\r\n\r\n<script >\r\nimport axios from 'axios';\r\nimport Midia from '../components/Midia.vue'\r\n\r\nexport default {\r\n  name: 'Home',\r\n\r\n  data(){\r\n    return{\r\n      busca:'',\r\n      midia: [],\r\n    }\r\n  },\r\n  //o created é carregado no inicio do carregamento da pagina\r\n  created: function(){\r\n    var key = process.env.VUE_APP_API_KEY;\r\n      axios.get(\"https://www.omdbapi.com/?s=avengers\"+key).then(res => {\r\n      console.log('app aqui!');\r\n      this.midia = res.data.Search;\r\n      console.log(this.midia)\r\n      \r\n      return this.midia\r\n    })\r\n    \r\n  },\r\n\r\n  components: {\r\n    Midia\r\n  },\r\n//toda vez que for buscado um filme esse método é usado\r\n  methods:{\r\n    buscar:function(){\r\n      var key = process.env.VUE_APP_API_KEY;\r\n      axios.get(\"https://www.omdbapi.com/?s=\"+this.busca+key).then(res => {\r\n      console.log('app aqui!');\r\n      if(res.data.Response != 'False'){\r\n        this.midia = res.data.Search;\r\n        console.log(this.midia)\r\n        return this.midia\r\n      }else{\r\n        window.alert(\"Filme nao encontrado\")\r\n      }\r\n    }) \r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n\r\n\r\n<style>\r\n#app {\r\n  font-family: Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n}\r\n\r\n#busca{\r\n  margin: 5% 20%;\r\n}\r\n\r\n.title span{\r\n  color: rgb(58, 248, 0);\r\n}\r\n\r\n.infobutt{\r\n  margin: 50px;\r\n}\r\n\r\n</style>\r\n"]}]}